<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.sust.po.User" table="user">
        <id name="id" column="user_id" type="java.lang.Integer">
            <generator class="native"></generator>
        </id>
        <property name="email" column="user_email" type="java.lang.String"></property>
        <property name="nickName" column="user_nickname" type="java.lang.String"></property>
        <property name="passWord" column="user_password" type="java.lang.String"></property>
        <property name="name" column="user_name" type="java.lang.String"></property>
    </class>
</hibernate-mapping>